  <data name="sql0001" xml:space="preserve">
    <value>select
T0.CardCode as 'Codigo',
T0.CardName as 'Nombre',
T0.DocEntry as 'Numero Documento',
T0.DocDate  as 'Fecha Documento',
T0.DocDueDate as 'Vencimiento',
abs(DATEDIFF(day,GETDATE(),T0.DocDueDate)) as 'Edad',
'N'as Checked, 
T0.DocTotal as 'Doc. Total',
T0.PaidToDate as 'Abono',
T0.DocTotal - T0.PaidToDate as 'Deuda Total',
0.0 as 'Indice',
0.0 as 'Valor Presente',
0.0 as 'Deterioro',
100.0 as 'Porcentaje',
0.0 as 'Total'
 from {0} T0
inner join OCRD on T0.CardCode = OCRD.CardCode
inner join OCRG on OCRG.GroupCode = OCRD.GroupCode
where (T0.DocStatus = 'O' and abs(DATEDIFF(day,GETDATE(),T0.DocDueDate)) &gt; 30)</value>
    <comment>Base DataTable Query</comment>
